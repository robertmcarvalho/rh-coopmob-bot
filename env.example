# .env.example

# n8n Basic Auth
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=seu_usuario_n8n
N8N_BASIC_AUTH_PASSWORD=sua_senha_segura

# Timezone
GENERIC_TIMEZONE=America/Sao_Paulo

# Redis URL (local) - in Render, use the Redis Instance URL
N8N_REDIS_URL=redis://redis:6379

# Twilio WhatsApp
TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_WHATSAPP_NUMBER=whatsapp:+1234567890

# Google Sheets
GOOGLE_SHEETS_CREDENTIALS_JSON='{"client_email":"...","private_key":"..."}'
SPREADSHEET_ID=your_spreadsheet_id

# Gemini AI
GEMINI_CREDENTIAL_ID=your_gemini_credential_id

# Redis AI Memory (if separate credential)
REDIS_CREDENTIAL_ID=your_redis_credential_id
